POP     LDR     R4,[SP]
;entry with object in R4
POPA    TEQ     R4,#TUNTIL
        ADDEQ   SP,SP,#8
        MOVEQ   PC,R14
        TEQ     R4,#TENDWH
        ADDEQ   SP,SP,#12
        MOVEQ   PC,R14
