        STR     IACC,[R4]              ;write back
INXTPR  TEQ     R6,#0                  ;test sign of step
        BMI     INXTCH
        CMP     IACC,R7                ;test if ascending loop
INXTMT  MOVLE   LINE,R5                ;if <= continue
